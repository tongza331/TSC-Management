<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record model="ir.rule" id="tsc_team_detail_team_public">
        <field name="name">TSC Detail: Team</field>
        <field name="model_id" ref="model_tsc_team"/>
        <field name="domain_force">[(1,'=',1)]</field>
        <field name="groups" eval="[(4, ref('base.group_public')), (4, ref('base.group_portal'))]"/>
    </record>

    <record model="ir.rule" id="tsc_team_detail_organization_public">
        <field name="name">TSC Detail: Organization</field>
        <field name="model_id" ref="model_tsc_organization"/>
        <field name="domain_force">[(1,'=',1)]</field>
        <field name="groups" eval="[(4, ref('base.group_public')), (4, ref('base.group_portal'))]"/>
    </record>

     <record model="ir.rule" id="tsc_team_detail_organization_public">
        <field name="name">TSC Detail: Career</field>
        <field name="model_id" ref="model_tsc_career"/>
        <field name="domain_force">[(1,'=',1)]</field>
        <field name="groups" eval="[(4, ref('base.group_public')), (4, ref('base.group_portal'))]"/>
    </record>

</odoo>
